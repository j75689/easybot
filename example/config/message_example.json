{
  "id": "MessageExample",
  "eventType": "message",
  "messageType": "text",
  "match": ["Hello", "hello", "Hello!", "hello!"],
  "timeout": 120,
  "defaultValues": {
    "repo": "https://github.com/j75689/easybot"
  },
  "stage": [
    {
      "type": "reply",
      "value": {
        "type": "text",
        "text": "Hello! I'm EasyBot. ${repo}"
      }
    }
  ]
}
